import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int sq = n * n;
        int temp = n;

        boolean isAuto = true;

        while (temp > 0) {
            if (temp % 10 != sq % 10) {
                isAuto = false;
                break;
            }
            temp = temp / 10;
            sq = sq / 10;
        }

        if (isAuto)
            System.out.println("Automorphic Number");
        else
            System.out.println("Not an Automorphic Number");
    }
}
